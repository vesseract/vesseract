{
  "name": "vesseract",
  "version": "0.0.0",
  "description": "Vesseract is an open source video game",
  "keywords": [],
  "homepage": "https://github.com/vesseract/vesseract#readme",
  "bugs": {
    "url": "https://github.com/vesseract/vesseract/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vesseract/vesseract.git"
  },
  "license": "MIT",
  "contributors": [
    "electrovir",
    "Jordan Last <jordan.michael.last@gmail.com>"
  ],
  "scripts": {
    "format:others": "prettier --ignore-path .gitignore --write './{,/**/}*.+(html)'",
    "lint": "eslint . --fix --ext .ts --ext .json --ext .js; prettier --loglevel warn --ignore-path .gitignore --write './{,/**/}*.+(html|md)'",
    "lint:check": "eslint . --ext .ts --ext .json --ext .js; prettier --ignore-path .gitignore --check './{,/**/}*.+(html|md)'",
    "signaling": "ts-node --transpile-only backend/signaling.ts",
    "start": "concurrently \"npm run zwitterion\" \"npm run signaling\"",
    "zwitterion": "zwitterion --port 4060"
  },
  "dependencies": {
    "lit-html": "1.2.1",
    "reduxular": "0.0.6",
    "ts-node": "8.10.1",
    "ws": "7.3.0"
  },
  "devDependencies": {
    "@types/ws": "7.2.4",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "concurrently": "5.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json-files": "0.8.1",
    "eslint-plugin-modules-newline": "0.0.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-typescript-sort-keys": "0.9.0",
    "eslint-plugin-unused-imports": "0.1.3",
    "prettier": "2.0.5",
    "zwitterion": "0.45.0"
  }
}